{
  "expo": {
    "name": "NETFLIX TOUNSI",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "scheme": "netflixtounsi",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "icon": "./assets/rounded_icon_1024.png",

    "splash": {
      "image": "./assets/rounded_splash_2000.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },

    "web": {
      "favicon": "./assets/rounded_favicon_64.png",
      "manifest": {
        "name": "NETFLIX TOUNSI",
        "short_name": "Tounsi",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#000000",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./assets/rounded_pwa_icon_192.png",
            "sizes": "192x192",
            "type": "image/png",
             "purpose": "maskable"
          },
          {
            "src": "./assets/rounded_pwa_icon_512.png",
            "sizes": "512x512",
            "type": "image/png",
             "purpose": "maskable"
          }
        ]
      }
    },

    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/rounded_android_foreground_512.png",
        "backgroundColor": "#000000"
      },
      "package": "com.ghassen_chebbi_7.myexpoapp",
      "userInterfaceStyle": "light"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ghassenchebbi.netflixtounsi",
      "userInterfaceStyle": "light",
      "infoPlist": {
        "UIStatusBarStyle": "UIStatusBarStyleBlackTranslucent",
        "UIRequiresFullScreen": true,
        "UIApplicationSupportsIndirectInputEvents": true
      }
    },

    "assetBundlePatterns": ["**/*"],

    "experiments": {
      "tsconfigPaths": true
    },

    "extra": {
      "eas": {
        "projectId": "7ea8b167-0826-4b11-84e6-7a6ea577d1e4"
      }
    },

    "owner": "ghassen_chebbi_7"
  }
}
